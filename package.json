{
  "name": "family-ledger",
  "version": "1.0.0",
  "description": "家庭记账本 - Electron 桌面应用",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:electron\"",
    "dev:renderer": "cd renderer && npm run dev",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "build": "cd renderer && npm run build"
  },
  "keywords": ["electron", "sqlite", "ledger", "家庭记账"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0"
  },
  "dependencies": {
    "sql.js": "^1.11.0"
  }
}
